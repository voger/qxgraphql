{
  "targets": [
    {
      "type": "source",
      "outputPath": "compiled/source",
      "bundle": {
        "include": "qx.*"
      }
    },

    {
      "type": "build",
      "outputPath": "compiled/build"
    }
  ],
  "defaultTarget": "source",
  "locales": ["en"],

  "applications": [
    {
      "class": "qxgraphql.demo.Application",
      "theme": "qxgraphql.theme.Theme",
      "name": "qxgraphql",
      "include": ["qxgraphql.GraphqlQueryCompress"],
      "bootPath": "source/boot"
    }
  ],
  "libraries": [ "../../../../opt/qooxdoo/framework", "." ]
}
